{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
        "source.fixAll": "always",
        "source.organizeImports": "always"
    },
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#4597b6",
        "activityBar.background": "#4597b6",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#8b3573",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#4597b6",
        "statusBar.background": "#377891",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#4597b6",
        "statusBarItem.remoteBackground": "#377891",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#377891",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#37789199",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#377891"
}
